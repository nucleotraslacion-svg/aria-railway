{
  "providerId": "boggad.pro",
  "providerName": "Boggad",
  "serviceId": "website",
  "serviceName": "Boggad Website",
  "version": 1,
  "syncPubKeyDomain": "boggad.pro",
  "logoUrl": "https://boggad.pro/logo.png",
